[MASTER]
load-plugins=pylint_pytest, pylint_django, pylint_celery
django-settings-module=adit.settings.development
ignore=migrations

[FORMAT]
max-line-length=100

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,too-many-lines,too-few-public-methods,duplicate-code,no-name-in-module

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete

[MISCELLANEOUS]
notes=FIXME,XXX